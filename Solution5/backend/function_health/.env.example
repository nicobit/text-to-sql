# Repository selection
CONFIG_REPOSITORY_KIND=blob
CONFIG_BLOB_ACCOUNT_URL=https://<your-account>.blob.core.windows.net
CONFIG_BLOB_CONTAINER=app-config
CONFIG_BLOB_NAME=services.json
# Optional: CONFIG_BLOB_CONNECTION_STRING=...

# For 'file' repo instead:
# CONFIG_REPOSITORY_KIND=file
# CONFIG_FILE_PATH=/home/site/wwwroot/services.json

DEFAULT_TIMEOUT_SECONDS=3.5
SERVICEBUS_NAMESPACE=<your-namespace>.servicebus.windows.net
BLOB_ENDPOINT=https://<your-account>.blob.core.windows.net
AI_SEARCH_ENDPOINT=https://<your-search>.search.windows.net
AI_SEARCH_INDEX=<your-index>
AZURE_OPENAI_ENDPOINT=https://<your-aoai>.openai.azure.com
AZURE_OPENAI_API_VERSION=2024-10-21
AOAI_LIVE_CALL=false
AZURE_OPENAI_DEPLOYMENT=
KEY_VAULT_URI=https://<your-vault>.vault.azure.net/
KEY_VAULT_TEST_SECRET_NAME=
